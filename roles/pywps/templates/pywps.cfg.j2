[metadata:main]
identification_title={{ wps_title }}
identification_abstract={{ wps_abstract }}
identification_keywords=PyWPS,WPS,OGC,processing,Birdhouse
identification_keywords_type=theme
identification_fees=NONE
identification_accessconstraints=NONE
provider_name={{ wps_provider_name }}
provider_url={{ wps_provider_url }}
contact_name=Lastname, Firstname
contact_position=Position Title
contact_address=Mailing Address
contact_city=
contact_stateorprovince=Administrative Area
contact_postalcode=Zip or Postal Code
contact_country=
contact_phone=+xx-xxx-xxx-xxxx
contact_fax=+xx-xxx-xxx-xxxx
contact_email=Email Address
contact_url=Contact URL
contact_hours=0:00-24:00
contact_instructions=During hours of service. Off on weekends.
contact_role=pointOfContact

[server]
url=http://{{ wps_hostname }}:{{ wps_port }}/wps
outputurl=http://{{ wps_hostname }}:{{ wps_port }}/outputs/{{ wps_name }}/
outputpath=/var/lib/pywps/outputs/{{ wps_name }}
workdir=/var/lib/pywps/tmp/{{ wps_name }}
maxsingleinputsize=30mb
maxrequestsize=30mb
maxprocesses=30
parallelprocesses=10

[logging]
level={{ wps_log_level }}
file=/var/log/pywps/{{ wps_name }}.log
database={{ wps_database }}
format=%(asctime)s] [%(levelname)s] %(message)s
